<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.androidbootcamp.coffeefinderapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".sevenstars"
            android:label="@string/title_activity_sevenstars" >
        </activity>
        <activity
            android:name=".DunkinDoughnuts"
            android:label="@string/title_activity_dunkin_doughnuts" >
        </activity>
        <activity
            android:name=".LaCheesecake"
            android:label="@string/title_activity_la_cheesecake" >
        </activity>
        <activity
            android:name=".Starbucks"
            android:label="@string/title_activity_starbucks" >
        </activity>
    </application>

</manifest>
